{
  "name": "regl-shape",
  "version": "1.0.2",
  "description": "2D shape shader for regl",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config dev/webpack.config.js",
    "build": "rimraf lib/ && tsc",
    "postbuild": "copyfiles -u 1 src/**/*.{vert,frag} lib",
    "release": "npm run build && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulkre/regl-shape.git"
  },
  "keywords": [
    "regl",
    "shape",
    "shader",
    "line",
    "stroke",
    "fill",
    "2d",
    "webgl"
  ],
  "author": "Paul Kretschel <paul.kretschel@icloud.com> (https://github.com/paulkre)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulkre/regl-shape/issues"
  },
  "homepage": "https://github.com/paulkre/regl-shape#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@types/earcut": "^2.1.1",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.3.0",
    "glslify-loader": "^2.0.0",
    "hsl-rgb": "^1.0.0",
    "html-webpack-plugin": "^4.3.0",
    "raw-loader": "^4.0.1",
    "regl": "^1.6.1",
    "release-it": "^13.6.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "array-bounds": "^1.0.1",
    "array-normalize": "^1.1.4",
    "color-normalize": "^1.5.0",
    "earcut": "^2.2.2"
  }
}
